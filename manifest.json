{
    "manifest_version": 2,
    "name": "Text History",
    "version": "1.0",
    "description": "Chrome extension to store typed text history",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "exclude_matches": [],
            "js": [
                "content.js"
            ]
        }
    ]
}